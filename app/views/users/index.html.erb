<h1>Welcome <%= current_user.first_name %>!</h1>

<h3>Projects</h3>
<% current_user.owned_projects.each do |project| %>
  <div>
    <%= link_to project.name, "/projects/#{project.id}" %>
  </div>
<% end %>

<h3>Pledged Projects</h3>
<% current_user.backed_projects.each do |project| %>
  <div>
    <%= link_to project.name, "/projects/#{project.id}" %>
  </div>
<% end %>

